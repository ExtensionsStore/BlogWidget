<config>
    <modules>
        <Aydus_BlogWidget>
            <version>0.1.0</version>
        </Aydus_BlogWidget>
    </modules>
    <phpunit>
        <suite>
            <modules>
                <Aydus_BlogWidget />
            </modules>
        </suite>
    </phpunit>  
    <frontend>      
    </frontend>    
    <global> 
        <blocks>
            <aydus_blogwidget>
