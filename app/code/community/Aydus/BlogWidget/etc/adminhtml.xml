<config>
    <acl>
        <resources>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <aydus_blogwidget translate="title">
                                        <title>Blog Widget Extension</title>
                                    </aydus_blogwidget>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>
</config>
